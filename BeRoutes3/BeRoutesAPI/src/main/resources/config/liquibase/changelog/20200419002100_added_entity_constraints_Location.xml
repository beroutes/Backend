<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Location.
    -->
    <changeSet id="20200419002100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="duration_id"
                                 baseTableName="location"
                                 constraintName="fk_location_duration_id"
                                 referencedColumnNames="id"
                                 referencedTableName="duration"/>

        <addForeignKeyConstraint baseColumnNames="photo_id"
                                 baseTableName="location"
                                 constraintName="fk_location_photo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="photo"/>

        <addForeignKeyConstraint baseColumnNames="country_id"
                                 baseTableName="location"
                                 constraintName="fk_location_country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"/>

        <addForeignKeyConstraint baseColumnNames="travel_route_id"
                                 baseTableName="location"
                                 constraintName="fk_location_travel_route_id"
                                 referencedColumnNames="id"
                                 referencedTableName="travel_route"/>

    </changeSet>
</databaseChangeLog>
